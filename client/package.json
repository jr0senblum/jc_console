{
  "name": "care-logistics-c3",
  "version": "1.2.0",
  "description": "The Care Logistics Clinical Care Coordinator application.",
  "scripts": {
    "start": "parallelshell \"npm run lint:tools\" \"npm run test:watch\" \"npm run open:src\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "npm run test && babel-node tools/build.js && npm run open:dist",
    "dist": "npm run prebuild && npm run test && babel-node tools/build.js",
    "storybook": "start-storybook -p 3020 -s ./src/shared",
    "storybook:dev": "parallelshell \"npm run lint:tools\" \"npm run test:watch\" \"npm run storybook\"",
    "test": "cross-env NODE_ENV=test mocha --require ./tools/mocha-init --reporter progress --compilers js:babel-core/register --recursive \"./src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "integration": "cross-env NODE_ENV=test mocha --require ./tools/mocha-init --timeout 5000 --reporter progress --compilers js:babel-core/register --recursive \"./src/**/*.spec.int.js\"",
    "integration:watch": "npm run integration -- --watch"
  },
  "author": "Care Logistics UI Team",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "babel-es6-polyfill": "1.0.1",
    "cl-ui-common": "0.1.0-3",
    "classnames": "2.2.5",
    "connect-history-api-fallback": "1.3.0",
    "event-source-polyfill": "0.0.7",
    "exports-loader": "0.6.3",
    "history": "^4.2.0",
    "immutable": "3.8.1",
    "imports-loader": "0.6.5",
    "material-ui": "0.15.4",
    "moment": "2.14.1",
    "object-assign": "4.1.0",
    "react": "15.3.1",
    "react-addons-css-transition-group": "15.3.1",
    "react-addons-update": "15.3.1",
    "react-dom": "15.3.1",
    "react-router": "^4.0.0-alpha.3",
    "react-tap-event-plugin": "^1.0.0",
    "victory": "^0.11.0"
  },
  "devDependencies": {
    "@kadira/storybook": "2.5.1",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.13.0",
    "browser-sync": "2.14.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "cross-env": "2.0.0",
    "css-loader": "0.24.0",
    "eslint": "3.3.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-react": "6.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "form-data": "1.0.0-rc4",
    "http-proxy": "1.14.0",
    "immutable-devtools": "0.0.7",
    "mocha": "3.0.2",
    "node-sass": "3.8.0",
    "parallelshell": "2.0.0",
    "react-addons-perf": "15.3.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "w3c-blob": "0.0.1",
    "watch": "0.19.2",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "yargs": "5.0.0"
  },
  "keywords:": [
    "care",
    "logistics",
    "c3"
  ],
